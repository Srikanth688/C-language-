#include<stdio.h>
    int main()
    {
        int i,j;
        int result=2;
        for(i=1;i<=6;i++)
        {
            for(j=1;j<=i;j++)
            {
                result += i;
                
                printf("%d%d",j,result);
                continue;
            }
            printf("%d\n",j);
        
        }
        return 0;
        
    }