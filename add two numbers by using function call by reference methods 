include<stdio.h>
    int sum(int *,int *);
    int main(){
        
        int x,y,result;
        
        printf("enter the value of x,y:\n");
        scanf("%d%d",&x,&y);
       result= sum(&x,&y);
        printf("the sum=%d",result);
    }
    
    int sum(int *a,int *b)
    {
        
        int c;
        c=*a+*b;
        return c;
    }
        