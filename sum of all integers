#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int sum=0;
    int i,n;
    scanf("%d",&n);
    int a[n];
    
    for(i=0;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<=n;i++)
    {
        sum+=a[i];
    }
    
printf("%d",sum);
return 0;
}
