#include<stdio.h>
int main(){
    int a[20];
    int *p[20];
    int i;
    printf("the array elements:");
    for(i=0;i<=10;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<=10;i++)
    {
         printf("%d\n",a[i]);
    }
    printf("the pointer variable:\n");
    for(i=0;i<=10;i++)
    {
        p[i]=&a[i];
    }
    
    for(i=0;i<=10;i++)
    {
        printf("%d\n",*p[i]);
    }
    
    return 0;
}
    
