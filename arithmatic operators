#include<stdio.h>
int main()
{
 int a=10,b=20,c;
printf("the value of a=%d,b=%d\n",a,b);
c=a+b;
printf("the addition operator c=%d\n",c);
c=a-b;
printf("the subtraction operator c=%d\n",c);
c=a*b;
printf("the multiplication operator c=%d\n",c);
c=a/b;
printf("the divison operator c=%d\n",c);
return 0;
}
