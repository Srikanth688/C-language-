
#include <stdio.h>
int main()
{
    int a[20][20];
    int i,j,r,c;
    printf("the rows amd columns:");
    scanf("%d%d",&r,&c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }

    
    for(i=0;i<r;i++)

    {
        for(j=0;j<c;j++)
        {
            printf("\t%d",a[i][j]);
        }
        printf("\n");
    }
    return 0;
    
}